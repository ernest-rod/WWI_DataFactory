{
	"name": "Stage Data From Containers",
	"properties": {
		"activities": [
			{
				"name": "Data Loader",
				"type": "ExecuteDataFlow",
				"dependsOn": [
					{
						"activity": "Get Event ID",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataflow": {
						"referenceName": "Load Data Into Stage Area",
						"type": "DataFlowReference"
					},
					"compute": {
						"coreCount": 8,
						"computeType": "General"
					}
				}
			},
			{
				"name": "Register Event",
				"type": "SqlServerStoredProcedure",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"storedProcedureName": "[dbo].[register_event]",
					"storedProcedureParameters": {
						"description": {
							"value": "Register Initial Process Event",
							"type": "String"
						},
						"is_error": {
							"value": "0",
							"type": "Boolean"
						},
						"parent_id": {
							"value": null,
							"type": "Int32"
						},
						"reporter": {
							"value": {
								"value": "@CONCAT( pipeline().DataFactory, '.', pipeline().Pipeline)",
								"type": "Expression"
							},
							"type": "String"
						},
						"unit_id": {
							"value": "1",
							"type": "Int32"
						}
					}
				},
				"linkedServiceName": {
					"referenceName": "AzureSqlM_Poseidon",
					"type": "LinkedServiceReference"
				}
			},
			{
				"name": "Get Event ID",
				"type": "Lookup",
				"dependsOn": [
					{
						"activity": "Register Event",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlMISource",
						"sqlReaderQuery": {
							"value": "SELECT CONCAT( 'dbo.get_event_id', '(', string(@pipeline().parameters.UnitID), ')' )",
							"type": "Expression"
						},
						"partitionOption": "None"
					},
					"dataset": {
						"referenceName": "AzureSqlMI_Poseidon_Query",
						"type": "DatasetReference",
						"parameters": {
							"Schema": "dbo",
							"Query": {
								"value": "@CONCAT( 'get_event_id', '(', string(pipeline().parameters.UnitID), ')' )",
								"type": "Expression"
							}
						}
					}
				}
			}
		],
		"parameters": {
			"ProcessingDate": {
				"type": "string"
			},
			"UnitID": {
				"type": "int"
			}
		},
		"annotations": []
	}
}